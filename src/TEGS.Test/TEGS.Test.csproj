<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>

    <IsPackable>false</IsPackable>

    <PackageLicenseFile>LICENSE.md</PackageLicenseFile>

    <RepositoryUrl>https://github.com/jonthysell/tegs</RepositoryUrl>

    <Authors>Jon Thysell</Authors>

    <Product>TEGS</Product>

    <Copyright>Copyright Â© 2019-2020 Jon Thysell</Copyright>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="TestGraphs\carwash.xml" />
    <None Include="..\..\LICENSE.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <Content Include="TestGraphs\carwash.xml">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.NET.Test.Sdk" Version="16.2.0" />
    <PackageReference Include="MSTest.TestAdapter" Version="2.0.0" />
    <PackageReference Include="MSTest.TestFramework" Version="2.0.0" />
    <PackageReference Include="coverlet.collector" Version="1.0.1" />
  </ItemGroup>

  <Import Project="..\TEGS\TEGS.projitems" Label="Shared" />

</Project>
